{
  "name": "@vramework/aws-services",
  "version": "0.5.2",
  "author": "yasser.fadl@gmail.com",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "type": "module",
  "scripts": {
    "tsc": "tsc",
    "build:esm": "tsc -b && echo '{\"type\": \"module\"}' > dist/esm/package.json",
    "build": "rm -rf dist && yarn build:esm",
    "release": "npm run build && npm test",
    "ncu": "ncu",
    "test": "echo 'No tests'"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./*": {
      "import": "./dist/*.js"
    },
    "./*.d.ts": {
      "import": "./dist/esm/*.d.ts"
    }
  },
  "peerDependencies": {
    "@vramework/core": "^0.5.10"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.676.0",
    "@aws-sdk/client-secrets-manager": "3.675.0",
    "@aws-sdk/s3-request-presigner": "3.676.0",
    "aws-cloudfront-sign": "^3.0.2"
  },
  "devDependencies": {
    "typescript": "^5.6"
  },
  "engines": {
    "node": ">=18"
  }
}
